<script type="application/javascript">
  $(document).ready(function(e){
    $(".box-footer").find('button').attr("disabled",'disabled');
    $(".btn-danger").removeAttr("onClick").attr("onClick",'alert("disabled");');
    $("#modal-danger, #modal-default").remove();
  });
  $(document).on('submit','.ajaxForm',function(e) {
    e.preventDefault();
    return false;
  });
</script>
  <%}%>
<script type="application/javascript">
$(document).on('click','.delete-selected',function(e){
    if($("tbody tr").find(".checkbox").is(":checked")){
        let itemArray = []
        $("tbody tr").find(".checkbox").each(function() 
        {
          if($(this).is(':checked') && $(this).val() != "on")
              itemArray.push($(this).val());
        });
        if(confirm("Are you sure you want to delete!!!"))
          window.location.href = "?ids="+itemArray.join(",")
    }else{
        alert("Please select entry to delete.");
    }
  })
  $(document).on('click','.checkall',function(e){
    if($(this).prop('checked')){
        $('tr').find(".checkbox").prop("checked", true);
    }else{
        $('tr').find(".checkbox").prop("checked", false);
    }
  })
</script>
